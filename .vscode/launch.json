    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Launch current leetcode solution",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/${fileDirnameBasename}/${fileDirnameBasename}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "internalConsoleOptions": "openOnSessionStart",
            "externalConsole": false,
            "MIMode": "lldb"
        }
    ]
}